<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="405.0" prefWidth="402.0" style="-fx-background-color: #fff;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginAdmin">
   <children>
      <Pane prefHeight="62.0" prefWidth="402.0" style="-fx-background-color: blue;">
         <children>
            <Label layoutX="34.0" layoutY="18.0" text="Admin Login" textFill="WHITE">
               <font>
                  <Font name="Bodoni MT Bold" size="24.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <ImageView fitHeight="129.0" fitWidth="131.0" layoutX="139.0" layoutY="74.0" pickOnBounds="true" preserveRatio="true" />
      <TextField fx:id="user" layoutX="52.0" layoutY="244.0" prefHeight="25.0" prefWidth="198.0" promptText="Username" />
      <PasswordField fx:id="pass" layoutX="52.0" layoutY="289.0" prefHeight="25.0" prefWidth="198.0" promptText="Password" />
      <Button fx:id="cancel" layoutX="52.0" layoutY="352.0" mnemonicParsing="false" onAction="#cancel" text="CANCEL" />
      <Button fx:id="login" layoutX="198.0" layoutY="352.0" mnemonicParsing="false" onAction="#login" style="-fx-background-color: blue;" text="LOGIN" textFill="WHITE" />
   </children>
</AnchorPane>
